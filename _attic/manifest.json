{
    "name" : "BOLDCODE",
    "description" : "Datatransition rgpd cleaner ",
    "version": "1.0",
    "manifest_version": 2,
    "author": "BC",

    "icons":{
      "16":"img/icon16.png",
      "128":"img/icon.png"
    },
    
    "browser_action":{
      "default_icon":{
      "16":"img/icon16.png"
    },
      
     "default_popup":"popup.html",
      "default_title":"Data Transition"
      },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ]
        }
      ],
      
    

  "permissions": [
      "tabs",
      "activeTab",
      "<all_urls>"
      
    ]
     
}